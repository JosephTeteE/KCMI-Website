{
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "camp.kcmi-rcc.org"
        }
      ],
      "destination": "/camp/$1"
    }
  ],
  "redirects": [
    {
      "source": "/youth-camp.html",
      "destination": "https://camp.kcmi-rcc.org",
      "permanent": true
    }
  ]
}
